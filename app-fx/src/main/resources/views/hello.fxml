<?import javafx.scene.control.Label?>
<?import javafx.scene.Scene?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<Scene xmlns:fx="http://javafx.com/fxml" fx:controller="cdu.HelloController">
    <VBox>
        <Button onAction="#changeToNorsk" text="Norsk"></Button>
        <Button onAction="#changeToEnglish" text="English"></Button>
        <Label text="%greeting"></Label>
        <stylesheets>
            <URL value="@../css/hello.css"></URL>
        </stylesheets>
    </VBox>

</Scene>